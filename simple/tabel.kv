<LabelValue>:
    font_name:"fonts/Monoid-Regular.ttf"
    font_size:self.height/2
    text:"-"
<LineHorizontal@Label>:
    size_hint:None,1
    pos_hint:{"center_x":.5,"center_y":.5}
    width:"1sp"
    canvas:
        Color:
            rgba:1,1,1,1
        Rectangle:
            size:self.size
            pos:self.pos
<Tabel>:
    rbv:rbv
    FloatLayout
        pos_hint:{"center_x":.5,"center_y":.5}
        canvas:
            Color:
                rgba:1,1,1,.6
            RoundedRectangle:
                size:self.size
                pos:self.pos
                source:"img/frame.png"

    BoxLayout
        pos_hint:{"center_x":.5,"center_y":.5}
        size_hint:.95,.92
        orientation:"vertical"
        BoxLayout:
            size_hint:1,.06
            Label:
                text:"No"
                font_size:self.height/1.5
                font_name:"fonts/Monoid-Retina.ttf"
                size_hint:.1,1
            LineHorizontal
            Label:
                text:"Item"
                font_size:self.height/1.5
                font_name:"fonts/Monoid-Retina.ttf"
            LineHorizontal
            Label:
                text:"Value"
                size_hint:.3,1
                font_size:self.height/1.5
                font_name:"fonts/Monoid-Retina.ttf"
            LineHorizontal
            Label:
                text:"Volt"
                size_hint:.3,1
                font_size:self.height/1.5
                font_name:"fonts/Monoid-Retina.ttf"
       
